<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <configSections>
    <section name="TargetSites" type="LocalDocs.Web.TargetSitesConfigSection" allowLocation="true" allowDefinition="Everywhere" />
  </configSections>
  <system.web>
    <httpHandlers>
      <add verb="*" path="*" type="LocalDocs.Web.MarkdownHttpHandler" />
    </httpHandlers>
    <compilation>
      <assemblies>
        <add assembly="System.Core, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" />
      </assemblies>
    </compilation>
  </system.web>
  <TargetSites>
    <add id="target1" name="Target Site 1" root="bin/MarkdownTestFiles_Target1/" isdefault="false" />
    <add id="target2" name="Second site" root="bin/MarkdownTestFiles_Target2/" isdefault="true" />
  </TargetSites>
</configuration>